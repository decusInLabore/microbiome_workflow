        } 
        
        pos <- c(
            grep("^PC1$", names(dfPCA)),
            grep("^PC2$", names(dfPCA))
        )
        
        if (length(pos) != 2){
            Obio@projectDetailList$dfPcaFN <- NULL
            print("Provided PCA file not valid. Check format of PCx columns. It should be PC1, PC2, ... Will use default calculation.")
        } 
        
